<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root prefHeight="500.0" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
    <children>
          <TitledPane animated="false" collapsible="false" text="Users" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <content>
                  <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="-1.0" prefWidth="-1.0">
                      <children>
                          <VBox prefHeight="-1.0" prefWidth="-1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                              <children>
                                  <ListView fx:id="userList" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" />
                                  <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" VBox.vgrow="NEVER">
                                      <items>
                                          <Button fx:id="deleteUserButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="-1.0" text="Delete" />
                                          <Button fx:id="editUserButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="-1.0" text="Edit" translateX="0.0" />
                                          <Button fx:id="newUserButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="-1.0" text="Add" />
                                      </items>
                                  </ToolBar>
                              </children>
                          </VBox>
                      </children>
                  </AnchorPane>
              </content>
          </TitledPane>
    </children>
</fx:root>
