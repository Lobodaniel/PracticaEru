<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TitledPane collapsible="false" text="Tags" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <content>
                <AnchorPane>
                    <children>
                        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <children>
                                <ToolBar>
                                    <items>
                                        <ChoiceBox fx:id="tagGroupsChoiceBox" prefWidth="150.0" />
                                        <Button fx:id="addGroupButton" mnemonicParsing="false" text="New" />
                                        <Button fx:id="deleteGroupButton" mnemonicParsing="false" text="Delete" />
                                    </items>
                                </ToolBar>
                                <TableView style="-fx-font-size: 12px" fx:id="tagsTableView" VBox.vgrow="ALWAYS">
                                    <columns>
                                        <TableColumn editable="false" maxWidth="5000.0" minWidth="10.0" prefWidth="50.0" text="Name">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="name" />
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn editable="false" maxWidth="5000.0" minWidth="10.0" prefWidth="106.0" text="Value">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="currentText" />
                                            </cellValueFactory>
                                        </TableColumn>
                                    </columns>
                                </TableView>
                                <HBox alignment="TOP_RIGHT" prefHeight="-1.0" prefWidth="-1.0" spacing="20.0">
                                    <children>
                                        <Button fx:id="editTagButton" layoutX="299.0" layoutY="20.0" mnemonicParsing="false" prefHeight="30.0" text="Edit" />
                                        <Button fx:id="addTagButton" mnemonicParsing="false" prefHeight="30.0" text="Add" />
                                        <Button fx:id="deleteTagButton" mnemonicParsing="false" prefHeight="30.0" text="Delete" />
                                    </children>
                                    <padding>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                    </padding>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
    </children>
</fx:root>
