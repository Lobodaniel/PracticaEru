<AnchorPane minHeight="-Infinity" minWidth="-Infinity" opacity="0.90" prefHeight="550.0" prefWidth="950.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.assemblits.eru.alarming.AlarmsController">
    <children>
        <VBox prefHeight="-1.0" prefWidth="-1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <ToolBar maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" VBox.vgrow="NEVER">
                    <items>
                        <Button alignment="CENTER" contentDisplay="CENTER" maxWidth="-Infinity" minHeight="-Infinity" mnemonicParsing="false" onAction="#clearAlarmsButtonFired" prefHeight="22.0" text="Acknowledge All Alarms" translateY="0.0">
                            <font>
                                <Font name="System Bold" size="12.0" />
                            </font>
                        </Button>
                        <Button alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="9.0" maxWidth="-Infinity" minHeight="-Infinity" mnemonicParsing="false" onAction="#exitButtonFired" prefHeight="22.0" text="Close Alarms">
                            <font>
                                <Font name="System Bold" size="12.0" />
                            </font>
                        </Button>
                    </items>
                </ToolBar>
                <TableView fx:id="table" disable="false" editable="false" focusTraversable="false" maxHeight="-1.0" maxWidth="-1.0" minHeight="-1.0" prefHeight="-1.0" prefWidth="-1.0" tableMenuButtonVisible="true" visible="true" VBox.vgrow="ALWAYS">
                </TableView>
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</AnchorPane>
